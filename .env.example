# .env.example

# API Keys
CENSUS_API_KEY=INSERT_YOUR_CENSUS_API_KEY_HERE
FEC_API_KEY=INSERT_YOUR_FEC_API_KEY_HERE
BLS_API_KEY=INSERT_YOUR_BLS_API_KEY_HERE
HUD_API_KEY=INSERT_YOUR_HUD_API_KEY_HERE
EPA_API_KEY=INSERT_YOUR_EPA_API_KEY_HERE

# Configuration Overrides
LOG_LEVEL=INFO
ENABLE_CACHING=True
CACHE_DIR=cache
CACHE_EXPIRATION_TIME=86400  # In seconds

ENABLE_PARALLELIZATION=True
NUM_WORKERS=4
TREND_YEARS=2000,2010,2020

ENABLE_ENCRYPTION=True
ENCRYPTION_KEY=INSERT_YOUR_SECURE_ENCRYPTION_KEY_HERE  # Ensure this is securely stored

# Flask Configuration
FLASK_HOST=127.0.0.1
FLASK_PORT=5000
FLASK_DEBUG=False

# Database Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# Optimization Parameters
GA_POPULATION_SIZE=200
GA_GENERATIONS=150
GA_CROSSOVER_RATE=0.9
GA_MUTATION_RATE=0.1
GA_SELECTION_METHOD=tournament  # Options: 'tournament', 'roulette', etc.
